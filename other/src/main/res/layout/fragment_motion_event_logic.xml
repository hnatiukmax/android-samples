<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".lib.MotionEventLogicFragment"
    tools:ignore="MissingDefaultResource">

    <dev.hnatiuk.android.samples.other.lib.FrameLayout1
        android:id="@+id/frame1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="20dp"
        android:background="#990E421B">

        <dev.hnatiuk.android.samples.other.lib.FrameLayout2
            android:id="@+id/frame2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginHorizontal="60dp"
            android:layout_marginVertical="100dp"
            android:background="#661B6398">

            <dev.hnatiuk.android.samples.other.lib.TopView
                android:id="@+id/topView"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_gravity="center"
                android:background="#B36E1919" />

        </dev.hnatiuk.android.samples.other.lib.FrameLayout2>

    </dev.hnatiuk.android.samples.other.lib.FrameLayout1>

</FrameLayout>